package com.learn.java;

import java.util.Arrays;
import java.util.List;

public class Main {
    public static void main(String[] args) {
    	List<Integer> list = Arrays.asList(3, 1, 6, 7, 9, 0);
    	list.stream()
    				.filter(t -> (t&1)==1)
    				.forEach(t->System.out.println(t));
    }
}
