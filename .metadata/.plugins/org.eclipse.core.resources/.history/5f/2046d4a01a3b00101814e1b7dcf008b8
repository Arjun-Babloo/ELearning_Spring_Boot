package com.learn.java;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class Main {
    public static void main(String[] args) {
    	int arr[] = {1,2,3,4,5};
    	List<Integer> list = Arrays.stream(arr)
    			.boxed()
    			.collect(Collectors.toList());
    	
    }
}
